    "clean-css": "^4.2.1",
    "cssnano": "^4.1.10",
    "dedent": "^0.7.0",
    "highlight.js": "^10.4.1",
    "jest": "^26.6.1",
    "jest-diff": "^27.3.1",
    "next": "^12.0.1",
    "postcss": "^8.2.3",
    "prettier": "^2.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
